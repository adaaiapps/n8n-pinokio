N8N_LOG_LEVEL=info

# Execution Timeout
N8N_EXECUTIONS_TIMEOUT=300

# Queue Mode
N8N_QUEUE_MODE=default

# Tunnel
N8N_TUNNEL_SUBDOMAIN=

# Other Settings
